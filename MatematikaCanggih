package interhance;

/**
 *
 * @author Sega Febrianargo Sejati
 */
public class Matematikacanggih {
    void modulus(int a, int b){
        System.out.println("modulus : "+a+"%"+b+"="+ (a%b) );
    }
    void pertambahan(int a, int b){
        System.out.println("pertambahan : "+a+"%"+b+"="+ (a%b));
    }
    void perkalian (int a, int b){
        System.out.println("perkalian : "+a+"%"+b+"="+ (a%b));
    }
}

package interhance;

/**
 *
 * @author Sega Febrianargo Sejati
 */
public class Matematikacanggihkrut {
    public static void main(String[] args) {
        Matematikacanggih aksimatematika = new Matematikacanggih();
        
        pusingmatematika.pertambahan(15,15);
        pusingmatematika.perkalian(5,8);
        pusingmatematika.modulus(10,200);
    }
}
